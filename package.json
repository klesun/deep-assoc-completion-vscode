{
	"name": "deep-assoc-completion-vscode",
	"publisher": "klesun",
	"author": {
		"name": "Artur Klesun",
		"email": "safronevev@gmail.com",
		"url": "https://klesun-productions.com/"
	},
	"license": "MIT",
	"displayName": "deep-assoc-completion-vscode",
	"description": "Completion and type inference of associative array keys",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.43.0"
	},
	"type": "module",
	"categories": [
		"Linters",
		"Programming Languages"
	],
	"keywords": [
		"associative arrays",
		"type inference",
		"annotate array keys",
		"php"
	],
	"activationEvents": [
		"onLanguage:php"
	],
	"main": "./src/main.js",
	"icon": "images/svg_256x256.png",
	"repository": {
		"type": "git",
		"url": "git@github.com:klesun/deep-assoc-completion-vscode.git"
	},

	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "deep-assoc-completion-vscode"
			}
		]
	},
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"typescript": "^3.7.5"
	},
	"dependencies": {
		"vscode-languageclient": "latest",
		"ts-node": "latest"
	}
}
